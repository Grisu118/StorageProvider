{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "trailing-comma": [true,  {"multiline": "never", "singleline": "never"}],
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "max-classes-per-file": false,
    "no-console": false
  },
  "rulesDirectory": []
}