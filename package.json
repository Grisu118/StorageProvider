{
  "name": "storageprovider",
  "version": "1.4.1",
  "description": "A simple API for accessing different Storages, including LocalStorage, SessionStorage and URL get parameters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Lightspots/StorageProvider",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "jest",
    "typedoc": "typedoc"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "storage",
    "localStorage",
    "sessionStorage"
  ],
  "author": "Grisu118",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testURL": "http://localhost/unitTest",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
