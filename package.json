{
  "name": "storageprovider",
  "version": "1.4.0",
  "description": "A simple API for accessing different Storages, including LocalStorage, SessionStorage and URL get parameters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Grisu118/StorageProvider",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project .",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "storage",
    "localStorage",
    "sessionStorage"
  ],
  "author": "Grisu118",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^8.0.57",
    "jest": "^23.6.0",
    "jest-localstorage-mock": "^2.3.0",
    "ts-jest": "^23.10.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testURL": "http://localhost/unitTest",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
