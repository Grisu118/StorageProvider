{
  "name": "storageprovider",
  "version": "1.2.0",
  "description": "A simple API for accessing different Storages, including LocalStorage, SessionStorage and URL get parameters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Grisu118/StorageProvider",
  "scripts": {
    "build": "tsc",
    "test": "jest"
  },
  "keywords": [
    "storage"
  ],
  "author": "Grisu118",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^8.0.57",
    "jest": "^23.1.0",
    "jest-localstorage-mock": "^2.2.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testURL": "http://localhost/unitTest",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  }
}
